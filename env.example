# База данных
DATABASE_URL="postgresql://postgres:password@localhost:5432/m2_rental"

# JWT
JWT_SECRET="your-super-secret-jwt-key-here"

# Логирование
LOG_LEVEL="INFO"

# Next.js
NEXT_TELEMETRY_DISABLED="1"

# Платежные провайдеры
PAYMENT_PROVIDER="mock" # mock, stripe, yoomoney, paypal
STRIPE_SECRET_KEY="sk_test_..."
STRIPE_PUBLISHABLE_KEY="pk_test_..."
YOOMONEY_SECRET_KEY="your-yoomoney-secret"

# Email провайдеры
EMAIL_PROVIDER="mock" # mock, sendgrid, nodemailer
SENDGRID_API_KEY="SG.your-sendgrid-key"
SMTP_HOST="smtp.gmail.com"
SMTP_PORT="587"
SMTP_USER="your-email@gmail.com"
SMTP_PASS="your-app-password"

# Push уведомления
FIREBASE_PROJECT_ID="your-firebase-project"
FIREBASE_PRIVATE_KEY="your-firebase-private-key"
FIREBASE_CLIENT_EMAIL="your-firebase-client-email"

# SMS провайдеры
SMS_PROVIDER="mock" # mock, twilio, smsru
TWILIO_ACCOUNT_SID="your-twilio-sid"
TWILIO_AUTH_TOKEN="your-twilio-token"
TWILIO_PHONE_NUMBER="+1234567890"

# Файловое хранилище
STORAGE_PROVIDER="local" # local, s3, cloudinary
AWS_ACCESS_KEY_ID="your-aws-access-key"
AWS_SECRET_ACCESS_KEY="your-aws-secret-key"
AWS_REGION="us-east-1"
AWS_S3_BUCKET="your-s3-bucket"

# Аналитика
GOOGLE_ANALYTICS_ID="GA-XXXXXXXXX"
GOOGLE_TAG_MANAGER_ID="GTM-XXXXXXX"

# Социальные сети
GOOGLE_CLIENT_ID="your-google-client-id"
GOOGLE_CLIENT_SECRET="your-google-client-secret"
FACEBOOK_APP_ID="your-facebook-app-id"
FACEBOOK_APP_SECRET="your-facebook-app-secret"

# Redis (для кэширования и сессий)
REDIS_URL="redis://localhost:6379"

# Внешние API
YANDEX_RENT_API_KEY="your-yandex-rent-api-key"
AVITO_API_KEY="your-avito-api-key"

# Безопасность
CORS_ORIGIN="http://localhost:3000"
RATE_LIMIT_WINDOW_MS="900000" # 15 минут
RATE_LIMIT_MAX_REQUESTS="100"

# Мониторинг
SENTRY_DSN="your-sentry-dsn"
LOGTAIL_TOKEN="your-logtail-token" 